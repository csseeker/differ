{
  "DifferLogging": {
    "MinimumLevel": "Information",
    "EnableFileLogging": true,
    "EnableConsoleLogging": false,
    "EnableDebugLogging": false,
    "LogDirectory": "%LOCALAPPDATA%\\Differ\\Logs",
    "LogFileName": "differ-.log",
    "RetainedFileCountLimit": 14,
    "FileSizeLimitBytes": 10485760,
    "RollingInterval": "Day",
    "UseSharedLogFile": true,
    "LevelOverrides": {
      "Microsoft": "Warning",
      "System": "Warning",
      "Differ.Core": "Information"
    }
  }
}
